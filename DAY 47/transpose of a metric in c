#include <stdio.h>

int main()
{
    int metrix1[3][3] = {
                           {1 , 2 , 3},
                           {3 , 8 , 6},
                           {9 , 7 , 5},
                        };
    
    for(int i = 0 ; i<3; i++)
        {
            for(int j = 0 ; j<3; j++)
            {
                printf(" %d ", metrix1[j][i]);
            }
            printf("\n");
        }
}
