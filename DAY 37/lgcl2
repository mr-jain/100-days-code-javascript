public class T{	
		
	public static void main(String[] args) {
	   A a = new B();
	   a.print();
	   System.out.print(a.i);
	}
	
 }
 
 class A{
	 int i =10 ;
	 public void print() {
		 System.out.print("Value-A");
	 }
 }
  class B extends A
  {
	  int i =12;
	  public void print()
	  {
		  System.out.print("Value-B");
	  }
  }

